        // Gets the import, sku, ssl, portnumber info from request

        var contentImport = '';

        var sku = '';       

        var serverName = '';

        var ssl = '';

        var portNumber = '';

        var isScopedView =  '';

        

        // Get the information from server including stings that need to be passed to js

        var hotfixUpdatesPresent = 'False'.toLowerCase();

        var doneStatus = 'Done';

        var failedStatus = 'Failed';

        var canceledStatus = 'Canceled';

        var locationRequiredText ='Please select a download location';



        var internetExplorer = false;

        var downloadManager;



        /// <summary>

        ///   Page load

        /// </summary>

        function pageLoad() {

            if (internetExplorer) {

                // Any IE specific initialization.

            } else {

                // Replace the placeholder <div> with a list of updates and their files (as links) for download.

                var downloadFilesDiv = $get('downloadFiles');

                if (downloadInformation != null && downloadInformation.length > 0) {

                    for (var updateIndex = 0; updateIndex < downloadInformation.length; updateIndex++) {

                        var updateElement = document.createElement("div");

                        updateElement.innerText = downloadInformation[updateIndex].enTitle;

                        updateElement.className = "textTopTitlePadding textBold textSubHeadingColor";

                        downloadFilesDiv.appendChild(updateElement);



                        var files = downloadInformation[0].files;

                        if (files != null && files.length > 0) {

                            var hr = document.createElement("hr");

                            downloadFilesDiv.appendChild(hr);



                            var hasMultipleLanguages = false;

                            var language = files[0].longLanguages;

                            for (var fileIndex = 1; fileIndex < files.length; fileIndex++) {

                                if (language !== files[fileIndex].longLanguages) {

                                    hasMultipleLanguages = true;

                                    break;

                                }

                            }



                            var lastDisplayedLanguage = "";

                            for (var fileIndex = 0; fileIndex < files.length; fileIndex++) {

                                if (hasMultipleLanguages && files[fileIndex].longLanguages !== lastDisplayedLanguage) {

                                    var languageDiv = document.createElement("div");

                                    var languageText = document.createTextNode(files[fileIndex].longLanguages);

                                    languageDiv.appendChild(languageText)

                                    if (lastDisplayedLanguage !== "") {

                                        languageDiv.style.marginTop = "10px";

                                    }

                                    downloadFilesDiv.appendChild(languageDiv);

                                    lastDisplayedLanguage = files[fileIndex].longLanguages;

                                }



                                var fileLinkDiv = document.createElement("div");

                                var fileLink = document.createElement("a");

                                var linkText = document.createTextNode(files[fileIndex].fileName);

                                fileLink.appendChild(linkText);

                                fileLink.title = files[fileIndex].fileName;

                                fileLink.href = files[fileIndex].url;

                                fileLinkDiv.appendChild(fileLink);

                                downloadFilesDiv.appendChild(fileLinkDiv);

                            }

                        }

                    }

                }

            }



            // Get the elements for those events need to be attached

            var closeButton =  $get('downloadSettingsCloseButton');

            var continueButton =  $get('downloadSettingsContinueButton');

            var hotfixContinueButton = $get('hotfixContinueButton');

            var hotfixCheckBox = $get('hotFixCheckBox');

            var eulaAcceptButton = $get('eulaAcceptButton');

            var eulaCancelButton = $get('eulaCancelButton'); 

            var email = $get('email');

            

            if (ssl == Microsoft.UpdateServices.Catalog.Constants.EmptyString) {

                ssl = Microsoft.UpdateServices.Catalog.Constants.ImportUsingSslDefault;

            }

                

            // If port number is empty then port number is 433 if ssl is true otherwise 80

            if (portNumber == Microsoft.UpdateServices.Catalog.Constants.EmptyString) {

                if(ssl == "true") {    

                    portNumber = Microsoft.UpdateServices.Catalog.Constants.PortNumberSsl;

                } else {

                    portNumber = Microsoft.UpdateServices.Catalog.Constants.PortNumber;

                }

             }

                                 

            var progressElements = null;

            if (internetExplorer) {

                // Builds the progess elements array which conatins the progress div, cancel image, progress text elements for the all updates that 

                // are downloading

                progressElements = new Array();

                var progressUpdateID;

                if (typeof (downloadInformation) != typeof (Microsoft.UpdateServices.Catalog.Constants.UndefinedValue) && downloadInformation.length > 0) {



                    for (i = 0; i < downloadInformation.length; i++) {

                        progressUpdateID = downloadInformation[i].updateID;

                        progressElements[progressUpdateID] = new Object();

                        progressElements[progressUpdateID].progressDiv = $get(progressUpdateID);

                        progressElements[progressUpdateID].progressText = $get(progressUpdateID + "_Text");

                        progressElements[progressUpdateID].progressImg = $get(progressUpdateID + "_Img");

                        progressElements[progressUpdateID].progressImgParent = progressElements[progressUpdateID].progressImg.parentElement;

                    }

                }

                downloadManager = new Microsoft.UpdateServices.Catalog.DownloadManager($get('MUCatalogWebControl'), new Sys.UI.Control($get('downloadSettingsFolderSelectionInput')));

            } else {

                downloadManager = new Microsoft.UpdateServices.Catalog.DownloadManager(null, null);

            }



            var browseButton = $get('downloadSettingsFolderSelectionButton');

            downloadManager.initialize(

                closeButton //Close button in the progress dialog

                ,continueButton //continue button in the Download settings dialog

                ,hotfixContinueButton //continue button in the hotfix dialog

                ,hotfixCheckBox //Hotfix check box

                ,eulaAcceptButton //accept button in the Eula dialog

                ,eulaCancelButton //cancel button in the Eula dialog

                ,$get("dialogMainBox")//parent div that contains all the dialog divs

                ,$get('downloadProgress')//the progess dialog top element

                ,$get('eulaNotice') //Eula dialog top elemnet

                ,$get("cancelAll")// cancell all element in the progress dialog

                ,$get('eulaTitles')//Eula titles div in the eula dialog

                ,$get('eulaExplain')//Eula explanation div, this is the div that is used to insert the eula frame before

                ,$get("downloadSuccess")//Download success information div in the progress dialog

                ,$get("downloadFailed")//Download Failed information div in the progress dialog

                ,$get("downloadSucessHeaderDetailBox")//Download success header div in the progress dialog

                ,$get("downloadProgressHeader")//Download progress header div in the progress dialog

                ,$get('downloadSettings')//download settings dialog top element

                ,$get('hotFixNotice')//hotfix notice top element

                ,$get('email')//hotfix email element

                ,$get('hotFixEmailWarning') //hotfix email warning.

                ,contentImport

                ,sku       

                ,serverName

                ,ssl

                ,portNumber

                ,hotfixUpdatesPresent

                ,doneStatus

                ,failedStatus

                ,canceledStatus

                ,locationRequiredText

                ,Microsoft.UpdateServices.Catalog.Constants.PageThanks

                ,Microsoft.UpdateServices.Catalog.Constants.PageDownloadInformation

                ,Microsoft.UpdateServices.Catalog.Constants.PageDownloadDialog

                ,Microsoft.UpdateServices.Catalog.Constants.PageErrorInformation

                ,Microsoft.UpdateServices.Catalog.Constants.DisabledCancelImage

                ,Microsoft.UpdateServices.Catalog.Constants.DisabledCancellAllImage

                , "flatGreenButton buttonPadding" //progress class

                , "flatRedButton buttonPadding" //failed class

                , "flatGreenButton buttonPadding" // completed class

                , "flatOrangeButton buttonPadding" //cancled class

                , "downloadDialogProgressText progressTextCommon"//Progress text class

                , "flatGrayButton cancelButtonInactive buttonPadding"//hotfix continue button disabled class

                , "flatBlueButton cancelButton buttonPadding"//hotfix continue button active class

                , "cancelTDDisabled"//cancel td disabled class

                , "cancelHeaderDisabled"//cancel all td Disabled Class

                , "cancelButtonInactive"//Cancel image inactive class

                , progressElements // Array of progess elemnts which contains progrees div, cancel image, progress text for all the downloading updates (null for non-IE)

                ,downloadInformation//array of download information

                ,eulaInfo//array of eula information

                ,isScopedView //coming from scoped view page or not

                ,$get('downloadSettingsFileAlert')

                ,$get('folderLengthAlert')

                ,minFilePathLength

                ,'To retry downloading the canceled or failed updates, you can add them back to your basket by clicking OK.' 

            );



            $addHandler(closeButton, "click", function () { window.close(); });



            if (internetExplorer) {

                $addHandler(browseButton, "click", function () {

                    downloadManager.recordDownloadFolder('Select the folder to which updates will be downloaded.');

                });



                $addHandler(continueButton, "click", function () { downloadManager.continueButtonClick(); });

                $addHandler(hotfixCheckBox, "click", function () { downloadManager.hotfixCheckBoxClick(); });

                $addHandler(hotfixContinueButton, "click", function () { downloadManager.hotfixContinueButtonClick(); });

                $addHandler(eulaAcceptButton, "click", function () { downloadManager.eulaAcceptButtonClick(); });

                $addHandler(eulaCancelButton, "click", function () { window.close(); });

                $addHandler(email, "keyup", function (e) { downloadManager.emailInputChanged(e); });



                // Attach onclick events to all the cancel buttons in the data grid.

                // The event handler for add cancel all is different from the 

                // remaining buttons.

                var updateDiv = $get('tableContainer');

                var imgElments = updateDiv.getElementsByTagName("img");



                for (i = 0; i < imgElments.length; i++) {

                    if (imgElments[i].id != "cancelAll") {

                        attachEventToInput(imgElments[i]);

                    }

                    else {

                        attachEventToCancelAll(imgElments[i]);

                    }

                }



                $addHandler(window, "beforeunload", function (e) { if (downloadManager.windowOnBeforeUnload()) { return 'Closing this window will cancel any download or imports in progress.'; } });

                $addHandler(window, "onunload", function (e) { downloadManager.windowUnload(); });

            }



            if (!internetExplorer) {

                closeButton.style.display = "inline";

                continueButton.style.display = "none";

            }



            window.focus();

        }



        /// <summary>

        ///   helper function that is called to attach events to the cancel all image

        /// </summary> 

        function attachEventToCancelAll(cancelAllImgElment) {

            cancelAllImgElment.attachEvent('onclick', cancelAllEventHelper);

            cancelAllImgElment.attachEvent('onkeydown',

                function () {

                    if (event.keyCode == Microsoft.UpdateServices.Catalog.Constants.EnterButtonKeyCode) {

                        cancelAllImgElment.click();

                    }

                }

            );

        }



        /// <summary>

        ///   helper function that is called to attach events to the cancel images

        /// </summary>

        function attachEventToInput(imgElments) {

            imgElments.attachEvent('onclick', cancelEventHelper);

            imgElments.attachEvent('onkeydown',

                function () {

                    if (event.keyCode == Microsoft.UpdateServices.Catalog.Constants.EnterButtonKeyCode) {

                        imgElments.click();

                    }

                }

            );

        }  



        /// <summary>

        ///   helper function for progress check

        /// </summary>

        function checkProgressHelper() {

            downloadManager.checkProgress();

        }



        /// <summary>

        ///   helper function for Child updates Import

        /// </summary>  

        function importChildUpdatesHelper() {

            downloadManager.importChildUpdates();

        }



        /// <summary>

        ///   helper function for parent updates Import

        /// </summary>         

        function importUpdatesHelper() {

            downloadManager.importUpdates();

        }



        /// <summary>

        ///   helper function to cancel the events on the image elemets

        /// </summary>

        function cancelEventHelper() {

            downloadManager.cancelDownload(event.srcElement);

        }



        /// <summary>

        ///   helper function for cancell all image element

        /// </summary>

        function cancelAllEventHelper() {

            downloadManager.cancelAll();

        }



        function goToErrorPage(errCode) {

            var windowName = errCode.toString().replace(/-/g, Microsoft.UpdateServices.Catalog.Constants.EmptyString);

            var qs = new Microsoft.UpdateServices.Catalog.QueryString(window.location.search);

            var errorPage = window.open(Microsoft.UpdateServices.Catalog.Constants.PageErrorInformation + qs.appendErrorsQS("?id=" + errCode),

                windowName, 'height=480,width=600,status=no,toolbar=no,menubar=no,location=no,scrollbars=yes,resizable=1 ');

            return false;

        }

    </script>

</head>

